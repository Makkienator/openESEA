aign(campaign)\">\r\n                        <p class=\"p-text-light\">{{ dateFixer(campaign.open_survey_date, 'MMMM Do YYYY') }} - {{ dateFixer(campaign.close_survey_date, 'MMMM Do YYYY') }}</p> <!-- MM/Do/YYYY' -->\r\n                    <img :src=\"campaign.image\" alt=\"Campaign Image\" style=\"max-width: 150px; max-height: 150px; border-radius: 50%;\" format=\"PNG\">\r\n                    <p class=\"p-text-italic\">{{campaign.name}}</p>\r\n                    <Divider />\r\n                    <div class=\"p-text-left p-ml-3\">\r\n                        <p>Method: <span class=\"p-text-bold\">{{campaign.method_name}}</span></p> <!-- Should be method name instead of pk! -->\r\n                        <p>Participating Organisations: <span class=\"p-text-bold\">{{campaign.organisation_accounts.length}}</span></p>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <DataTable v-else :value=\"filteredCampaigns\" datakey=\"id\" selectionMode=\"single\" @row-select=\"goToCampaign\" showGridlines autoLayout\r\n        :paginator=\"true\" :rows=\"10\" :filters=\"filters\" paginatorTemplate=\"FirstPageLink PrevPageLink PageLinks NextPageLink LastPageLink CurrentPageReport RowsPerPageDropdown\"\r\n        :rowsPerPageOptions=\"[5,10,25]\" currentPageReportTemplate=\"Showing {first} to {last} of {totalRecords} products\" class=\"p-datatable-striped\">\r\n\r\n            <Column v-for=\"col of columns\" :field=\"col.field\" :header=\"col.header\" :key=\"col.field\" bodyStyle=\"\" />\r\n        </Datatable>\r\n    </div>\r\n    <div v-else class=\"p-text-italic\">No Campaigns to display!</div>\r\n</template>\r\n\r\n<script>\r\n    import { mapState, mapActions } from 'vuex'\r\n    import ListBar from '@/components/lists/ListBar/'\r\n    import ProgressSpinner from 'primevue/progressspinner'\r\n    import dateFixer from '../../utils/datefixer'\r\n\r\n    export default {\r\n        components: {\r\n            ListBar,\r\n            ProgressSpinner\r\n        },\r\n        data () {\r\n            return {\r\n                tableDisplay: false,\r\n                loading: true,\r\n                failedLoad: false,\r\n                customSearch: '',\r\n                allCampaigns: false,\r\n                styleObject: { backgroundColor: '#EFEEEE', cursor: 'pointer' },\r\n                columns: [\r\n                    { field: 'name', header: 'Name' },\r\n                    { field: 'method_name', header: 'Method' },\r\n                    { field: 'organisation_accounts.length', header: 'Organisations' }\r\n                ]\r\n            }\r\n        },\r\n        computed: {\r\n            ...mapState('network', ['network']),\r\n            ...mapState('campaign', ['campaigns']),\r\n            filteredCampaigns () {\r\n                return this.campaigns.filter(campaign => {\r\n                    return (\r\n                        campaign.name.toLowerCase().includes(this.customSearch.toLowerCase()) ||\r\n                        campaign.method_name.toLowerCase().includes(this.customSearch.toLowerCase())\r\n                    )\r\n                })\r\n            }\r\n        },\r\n        created () {\r\n            this.getCampaigns()\r\n            setTimeout(() => { this.failedLoad = true }, 10000)\r\n        },\r\n        methods: {\r\n            ...mapActions('campaign', ['fetchCampaigns', 'setCampaign']),\r\n            dateFixer,\r\n            async getCampaigns () {\r\n                await this.fetchCampaigns({ nId: this.$route.params.NetworkId })\r\n                this.loading = false\r\n            },\r\n            async goToCampaign (campaign) {\r\n                if (campaign?.data) {\r\n                    campaign = campaign.data\r\n                }\r\n                if (campaign?.id) {\r\n                    await this.setCampaign(campaign)\r\n                    this.$router.push({ name: 'networkcampaign', params: { CampaignId: campaign.id } })\r\n                }\r\n            }\r\n        }\r\n    }\r\n</script>\r\n"],"mappings":";;;EAO2CA,KAAK,EAAC;;;;;;;EAEhBA,KAAK,EAAC;;;;EAGZA,KAAK,EAAC;AAAc;;;EAExBA,KAAK,EAAC;AAAe;;EAEnBA,KAAK,EAAC;AAAoB;;EACVA,KAAK,EAAC;AAAa;;EACEA,KAAK,EAAC;AAAa;;;EAYjEA,KAAK,EAAC;;;;;;;;6DA3BlBC,YAAA,CAEWC,mBAAA;IAFOC,YAAY,EAAEC,KAAA,CAAAC,YAAY;iEAAZD,KAAA,CAAAC,YAAY,GAAAC,MAAA;IAAUC,QAAQ,EAAEH,KAAA,CAAAI,YAAY;6DAAZJ,KAAA,CAAAI,YAAY,GAAAF,MAAA;IAAGG,eAAe,EAAE,KAAK;IAAUC,MAAM,EAAEN,KAAA,CAAAO,YAAY;2DAAZP,KAAA,CAAAO,YAAY,GAAAL,MAAA;IAAEM,IAAI,EAAC;;sBACtI,MAAa,CAAbC,WAAA,CAAaC,IAAA,CAAAC,MAAA,a;;6DAEMX,KAAA,CAAAY,OAAO,KAAKZ,KAAA,CAAAa,UAAU,I,cAA7CC,YAAA,CAAiDC,0BAAA;IAAAC,GAAA;EAAA,MACjChB,KAAA,CAAAY,OAAO,IAAIZ,KAAA,CAAAa,UAAU,I,cAArCI,mBAAA,CAAoG,OAApGC,UAAoG,EAAvC,mCAAiC,KAC9ER,IAAA,CAAAS,SAAS,CAACC,MAAM,I,cAAhCH,mBAAA,CAqBM,OAAAI,UAAA,GApBSrB,KAAA,CAAAC,YAAY,I,cAAvBgB,mBAAA,CAaM,OAbNK,UAaM,I,kBAZFL,mBAAA,CAWMM,SAAA,QAAAC,WAAA,CAXkBC,QAAA,CAAAC,iBAAiB,EAA7BC,QAAQ;yBAApBV,mBAAA,CAWM;MAXsCD,GAAG,EAAEW,QAAQ,CAACnB,IAAI;MAAEZ,KAAK,EAAC,wBAAwB;MAACgC,KAAoB,EAApB;QAAA;MAAA;QAC3FC,mBAAA,CASM;MATDjC,KAAK,EAAAkC,eAAA,EAAC,OAAO,EAASH,QAAQ,CAACI,KAAK;MAA4CH,KAA0B,EAAAI,eAAA,EAA1B;QAAA;MAAA,CAA0B,EAAUL,QAAQ,CAACI,KAAK,GAAG/B,KAAA,CAAAiC,WAAW;MAAUC,WAAS,EAAAhC,MAAA,IAAEyB,QAAQ,CAACI,KAAK;MAAQI,YAAU,EAAAjC,MAAA,IAAEyB,QAAQ,CAACI,KAAK;MAAWK,OAAK,EAAAlC,MAAA,IAAEuB,QAAA,CAAAY,YAAY,CAACV,QAAQ;QACzPE,mBAAA,CAAoJ,KAApJS,UAAoJ,EAAAC,gBAAA,CAAzHd,QAAA,CAAAe,SAAS,CAACb,QAAQ,CAACc,gBAAgB,qBAAoB,KAAG,GAAAF,gBAAA,CAAGd,QAAA,CAAAe,SAAS,CAACb,QAAQ,CAACe,iBAAiB,mCAAyBC,mBAAA,iBAAoB,EAC7Kd,mBAAA,CAA8H;MAAxHe,GAAG,EAAEjB,QAAQ,CAACkB,KAAK;MAAEC,GAAG,EAAC,gBAAgB;MAAClB,KAAgE,EAAhE;QAAA;QAAA;QAAA;MAAA,CAAgE;MAACmB,MAAM,EAAC;yCACxHlB,mBAAA,CAA8C,KAA9CmB,UAA8C,EAAAT,gBAAA,CAAnBZ,QAAQ,CAACnB,IAAI,kBACxCX,YAAA,CAAWoD,kBAAA,GACXpB,mBAAA,CAGM,OAHNqB,UAGM,GAFFrB,mBAAA,CAAwE,Y,2CAArE,UAAQ,IAAAA,mBAAA,CAAyD,QAAzDsB,UAAyD,EAAAZ,gBAAA,CAA7BZ,QAAQ,CAACyB,WAAW,iB,GAAcT,mBAAA,0CAA6C,EACtHd,mBAAA,CAA8G,Y,2CAA3G,+BAA6B