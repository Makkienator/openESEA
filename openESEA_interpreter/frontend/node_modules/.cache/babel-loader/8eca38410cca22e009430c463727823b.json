ge > 1) {\r\n                    this.currentPage--\r\n                }\r\n                if (direction === 'right' && this.currentPage < this.itemPages) {\r\n                    this.currentPage++\r\n                }\r\n                this.changeDisplayedItems()\r\n            },\r\n            goToItem (item) {\r\n                this.$emit('clicked-item', item, this.name.toLowerCase())\r\n            }\r\n        }\r\n    }\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n    .scrollIcon {\r\n        color: grey;\r\n        &:hover {\r\n            background-color: #EFEEEE;\r\n            color: black;\r\n        }\r\n    }\r\n</style>\r\n"],"mappings":";;EAGSA,KAAK,EAAC,wBAAwB;EAACC,KAAkC,EAAlC;IAAA;EAAA;;;EAC3BD,KAAK,EAAC;AAAuB;;EAC1BA,KAAK,EAAC;AAAa;;;;;EAGtBA,KAAK,EAAC,mCAAmC;EAACC,KAAmC,EAAnC;IAAA;IAAA;EAAA;;;EACtCA,KAAoB,EAApB;IAAA;EAAA;AAAoB;;;EAIcD,KAAK,EAAC;;;;EACfE,GAAG,EAAC,QAAQ;EAACF,KAAK,EAAC,QAAQ;EAACC,KAAoB,EAApB;IAAA;EAAA;;;;;;EAG1BD,KAAK,EAAC,aAAa;EAACC,KAAoB,EAApB;IAAA;EAAA;;;;EACjBD,KAAK,EAAC,aAAa;EAACC,KAAoB,EAApB;IAAA;EAAA;;;;EAI5BD,KAAK,EAAC;;;;;;EAM5BC,KAAoB,EAApB;IAAA;EAAA;AAAoB;;;;uBAzBjCE,mBAAA,CA6BM,OA7BNC,UA6BM,GA5BFC,mBAAA,CAGM,OAHNC,UAGM,GAFFD,mBAAA,CAAkO,MAAlOE,UAAkO,GAAvLC,IAAA,CAAAC,SAAS,I,cAA5BC,YAAA,CAAyKC,sBAAA;;IAA1IC,EAAE;MAAAC,IAAA,EAASL,IAAA,CAAAC,SAAS;MAAAK,MAAA;QAAAC,SAAA,EAAuBP,IAAA,CAAAQ,MAAM,CAACF,MAAM,CAACC;MAAS;IAAA;IAAMd,KAA2C,EAA3C;MAAA;MAAA;IAAA;;sBAA4C,MAAQ,C,kCAANgB,MAAA,CAAAJ,IAAI,iB;;8CAAgBV,mBAAA,CAA4B,QAAAe,UAAA,EAAAC,gBAAA,CAAbF,MAAA,CAAAJ,IAAI,kB,GACpNR,mBAAA,CAA8C,YAA1C,OAAK,GAAAc,gBAAA,CAAEC,KAAA,CAAAC,WAAW,IAAE,MAAI,GAAAF,gBAAA,CAAEG,QAAA,CAAAC,SAAS,iB,GAE3ClB,mBAAA,CAuBM,OAvBNmB,UAuBM,GAtBFnB,mBAAA,CAEM,OAFNoB,UAEM,GADSL,KAAA,CAAAC,WAAW,iB,cAAtBlB,mBAAA,CAAyK;;IAApIH,KAAK,EAAC,qBAAqB;IAAE0B,OAAK,EAAAC,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAAEN,QAAA,CAAAO,WAAW;gCAAWxB,mBAAA,CAAmE;IAAhEL,KAAK,EAAC,8BAA8B;IAACC,KAAwB,EAAxB;MAAA;IAAA;wEAEpHmB,KAAA,CAAAU,OAAO,KAAKV,KAAA,CAAAW,UAAU,I,cAA7CrB,YAAA,CAA0FsB,0BAAA